<?xml version="1.0"?>
<package xmlns="http://schemas.microsoft.com/packaging/2010/07/nuspec.xsd">
  <metadata>
    <id>laget.HashId</id>
    <title>laget.HashId</title>
    <version>$version$</version>
    <authors>gonace,laget.se</authors>
    <owners>laget.se</owners>
    <requireLicenseAcceptance>false</requireLicenseAcceptance>
    <description>An implementation for hashed ids to be used in public facing DTOs</description>
    <summary></summary>
    <license type="expression">Apache-2.0</license>
    <projectUrl>https://laget.se</projectUrl>
    <icon>assets\icon.png</icon>
